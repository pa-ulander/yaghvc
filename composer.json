{
    "name": "laravel/laravel",
    "type": "project",
    "version": "v11.0.0",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "badges/poser": "v3.1.0",
        "laravel/framework": "v11.41.3",
        "laravel/sanctum": "v4.0.8",
        "laravel/tinker": "v2.10.1",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "fakerphp/faker": "v1.24.1",
        "larastan/larastan": "v3.0.2",
        "laravel/pint": "v1.20.0",
        "laravel/sail": "v1.41.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "8.5.0",
        "pestphp/pest": "v3.7.1",
        "pestphp/pest-plugin-type-coverage": "v3.5.0",
        "phpunit/phpunit": "^11.5.1",
        "spatie/laravel-ignition": "^2.9.0",
        "squizlabs/php_codesniffer": "3.11.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test:lint": "vendor/bin/phpcs --standard=ruleset.xml ./app",
        "test:lint-fix": "vendor/bin/phpcbf --standard=ruleset.xml ./app",
        "test:analyze": "vendor/bin/phpstan analyse --memory-limit=2G",
        "test:unit": [
            "vendor/bin/pest tests/Unit"
        ],
        "test:feature": [
            "vendor/bin/pest tests/Feature"
        ],
        "test:coverage": [
            "vendor/bin/pest tests --coverage-clover clover.xml"
        ],
        "test:typecoverage": [
            "vendor/bin/pest tests --type-coverage --min=100"
        ],
        "test:update": [
            "vendor/bin/pest -d --update-snapshots"
        ],
        "test": [
            "@putenv XDEBUG_MODE=coverage",
            "@test:lint-fix",
            "@test:analyze",
            "@test:unit",
            "@test:feature",
            "@test:coverage",
            "@test:typecoverage",
            "@putenv XDEBUG_MODE=off"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
