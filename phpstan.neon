includes:
    - vendor/larastan/larastan/extension.neon

parameters:

    paths:
        - app/
        - routes/

    # Level 9 is the highest level
    level: 9

#    ignoreErrors:
#        - '#PHPDoc tag @var#'

    excludePaths:
        - ./storage?
        - ./routes/console.php

services:
    -
        class: Yagvc\PHPStan\Rules\NoNamedArgumentsOnFunctionsRule
        tags:
            - phpstan.rules.rule